{
    "name": "ts-archetype",
    "version": "0.2.4",
    "author": "Andreas Arvidsson",
    "license": "MIT",
    "main": "./lib/index.js",
    "types": "lib/types/index.d.ts",
    "files": [
        "lib/*",
        "!lib/scripts/*",
        "!lib/types/scripts/*",
        "resources/*"
    ],
    "homepage": "https://github.com/AndreasArvidsson/ts-archetype",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndreasArvidsson/ts-archetype.git"
    },
    "bugs": {
        "url": "https://github.com/AndreasArvidsson/ts-archetype/issues"
    },
    "scripts": {
        "compile": "tsc -p .",
        "generate-examples": "tsx src/scripts/generateExamples.ts && prettier --write examples",
        "fix:meta": "prettier --write .",
        "test:meta": "eslint src && prettier --check ."
    },
    "dependencies": {
        "file-updater": "0.4.0"
    },
    "devDependencies": {
        "@types/node": "^18.15.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "^8.36.0",
        "prettier": "^2.8.7",
        "tsx": "^3.12.6",
        "typescript": "^5.0.2"
    }
}
